{
  "manifest_version": 3,
  "name": "Take-Note",
  "version": "1.0",
  "permissions": ["tabs", "storage", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
